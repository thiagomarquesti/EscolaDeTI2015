	<parameter name="idconvenio" class="java.lang.Long"/>
	<parameter name="idadeini" class="java.lang.Integer"/>
	<parameter name="idadefim" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT tb.*
  FROM (SELECT row_number() OVER() AS coluna,
               i.codigoassindi,
               i.nome,
               i.genero,
               (EXTRACT(YEAR FROM CURRENT_DATE) - EXTRACT(YEAR FROM COALESCE(i.datanascimento, CURRENT_DATE))) AS idade
          FROM indigena i
          LEFT JOIN familia f ON (i.codigoassindi = f.idrepresentante)
          LEFT JOIN familia_indigena fi ON (fi.codigoassindi = i.codigoassindi)
          LEFT JOIN estadia_indigena ei ON (i.codigoassindi = ei.codigoassindi)
          LEFT JOIN estadia e ON (ei.idestadia = e.idestadia)
          LEFT JOIN indigena_convenio ic ON (i.codigoassindi = ic.indigena_id)
         WHERE (($P{genero} IS NULL) OR (i.genero = $P{genero}))
           AND (($P{dataini} IS NULL AND $P{datafim} IS NULL) OR (e.dataentrada BETWEEN $P{dataini} AND $P{datafim}) OR (e.datasaida BETWEEN $P{dataini} AND $P{datafim}))
           AND (($P{idfamilia} IS NULL) OR (fi.idfamilia = $P{idfamilia}) OR (f.idfamilia = $P{idfamilia}))
           AND (($P{idetnia} IS NULL) OR (i.etnia_idetnia = $P{idetnia}))
           AND (($P{escolaridade} IS NULL) OR (i.escolaridade = $P{escolaridade}))
           AND (($P{estadocivil} IS NULL) OR (i.estadocivil = $P{estadocivil}))
           AND (i.terraindigena_idterraindigena IN ($P!{terrasindigena}))
          
           
           AND (($P{idconvenio} IS NULL) OR (ic.convenio_id = $P{idconvenio}))
          GROUP BY i.codigoassindi                        
       ) AS tb
 WHERE (($P{idadeini} IS NULL AND $P{idadefim} IS NULL) OR (tb.idade BETWEEN $P{idadeini} AND $P{idadefim}))]]>
	</queryString>
